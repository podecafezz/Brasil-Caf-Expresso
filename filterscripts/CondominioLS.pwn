public OnPlayerCommandText(playerid, cmdtext[])
{
//==============================================================================
	if(strcmp("/ac", cmdtext, true) == 0)	{
    MoveObject(PC,836.445,-2065.518,19.641, 2.0);//19.641
	SendClientMessage(playerid,0x0000FFAA, "Abrindo o Portão do Condominio.");
   	return 1;
}
	if(strcmp("/fc", cmdtext, true) == 0)	{
    MoveObject(PC, 836.445,-2065.518,14.641, 2.0);
	SendClientMessage(playerid,0x0000FFAA, "Fechando o Portão do Condominio.");
   	return 1;
}
	if(strcmp("/ac2", cmdtext, true) == 0)	{
    MoveObject(PB, 808.732,-2220.295,1.815, 2.0);
    MoveObject(PB2, 808.732,-2200.250,1.815, 2.0);
	SendClientMessage(playerid,0x0000FFAA, "Abrindo o Portão do Condominio.");
   	return 1;
}
	if(strcmp("/fc2", cmdtext, true) == 0)	{
    MoveObject(PB, 808.732,-2252.371,1.815, 2.0);
    MoveObject(PB2, 808.732,-2232.343,1.815, 2.0);
	SendClientMessage(playerid,0x0000FFAA, "Fechando o Portão do Condominio.");
   	return 1;
}
//==============================================================================
	if(strcmp("/se", cmdtext, true) == 0)	{
    MoveObject(ELEV, 931.759,-2232.181,11.345, 2.0);
	SendClientMessage(playerid,verde, "Subindo Elevador...");
   	return 1;
}
	if(strcmp("/de", cmdtext, true) == 0)	{
   	SendClientMessage(playerid,azul, "Descendo Elevador...");
   	MoveObject(ELEV, 931.759,-2232.181,0.345, 2.0);
   	return 1;
}

//=============================================================================
	if(strcmp("/comandoscondominio", cmdtext, true) == 0)	{
    SendClientMessage(playerid,verde, "COMANDOS");
	SendClientMessage(playerid,amarelo, "Abrir e Fechar portão principal: /ac, /fc");
	SendClientMessage(playerid,amarelo, "Abrir e Fechar portão dos Barcos: /ac2, /fc2");
	SendClientMessage(playerid,amarelo, "Subir e Descer Elevador: /se, /de");
	SendClientMessage(playerid,verde, "Creditos: Lucaas_Caam e Renahn_xD");
   	return 1;
}
	return 0;
}
