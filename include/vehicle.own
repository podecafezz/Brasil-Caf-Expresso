	samp_mysql_store_result();
 	while(samp_mysql_fetch_row(resultline)==1)
	{
		samp_mysql_strtok(field, "|", resultline); //first call of strtok MUST contain resultline
		id = strval(field);
		while(samp_mysql_strtok(field2, "|", "")==1) {//From 2nd call to the end you MUST call strtok without resultline
		    ct++;
		    ct%=9;
		    if(ct == 1) modelid = strval(field2);
			if(ct == 2) x = Float:floatstr(field2);
			if(ct == 3) y = Float:floatstr(field2);
			if(ct == 4) z = Float:floatstr(field2);
			if(ct == 5) ang = Float:floatstr(field2);
			if(ct == 6) cor1 = strval(field2);
			if(ct == 7) cor2 = strval(field2);
			if(ct == 8) set(don,field2);
			if(ct == 0) gaso = strval(field2);
		}
		switch(modelid) {
		    case 481,509,510,594,599,523: GPV[id]=-1;
			default: GPV[id] = gaso;
		}
		set(Dono[id],don);
		new c=CreateVehicle(modelid,x,y,z,ang,cor1,cor2,-1);
		VCl[c][0]=cor1;
		VCl[c][1]=cor2;
		printf("Criado ID %d : modelo %d",c,modelid);
		SetTimerEx("CarregarTuning",6000,false,"d",c);
		SetTimerEx("CarregarPlaca",6000,false,"d",c);
	}
	if(!id) {
	    print("Não há nenhum carro nesse servidor, os carros serão automaticamente gerados");
	    GerarCarrosIniciais();
	    return 0;
	}
	LASTID = id;
	return 1;
}

stock GerarCarrosIniciais()	{
	for(new c=1; c<146; c++) CriarDocumentacao(InitialCars[c][mid],InitialCars[c][cx],InitialCars[c][cy],InitialCars[c][cz],InitialCars[c][cang],InitialCars[c][c1],InitialCars[c][c2],InitialCars[c][cgas],InitialCars[c][cdono],PlacasArray[c],InitialCars[c][cid]);
	CarregarCarros();
}

